{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 450,
  "title": "Australian Airport Traffic for Jan 2024",
  "projection": {"type": "mercator"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/JamesCo24/Vega-Lite/refs/heads/main/3179W9HW/js/AusMapWithGraticules.json",
        "format": {"type": "topojson", "feature": "GCCSA_2021_AUST_GDA2020"}
      },
      "mark": {"type": "geoshape", "fill": "darkseagreen", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/JamesCo24/Vega-Lite/refs/heads/main/3179W9HW/js/AusMapWithGraticules.json",
        "format": {"type": "topojson", "feature": "ne_110m_graticules_10"}
      },
      "mark": {"type": "geoshape", "stroke": "gray"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/JamesCo24/Vega-Lite/refs/heads/main/3179W9HW/data/Airport_Traffic.csv"
      },
      "transform": [{"filter": "datum.Month == '1'"}],
      "mark": {"type": "circle"},
      "encoding": {
        "longitude": {"field": "longitude", "type": "quantitative"},
        "latitude": {"field": "latitude", "type": "quantitative"},
        "size": {
          "field": "Pax_Total",
          "type": "quantitative",
          "title": "Airport Traffic",
          "scale": {
            "type": "sqrt",
            "domain": [40000, 3600000],
            "range": [50, 500]
          }
        },
        "tooltip": [
          {"field": "AIRPORT", "type": "nominal", "title": "Airport"},
          {"field": "Pax_In", "type": "quantitative", "title": "No. Arrivals"},
          {
            "field": "Pax_Out",
            "type": "quantitative",
            "title": "No. Departures"
          }
        ],
        "color": {"value": "darkblue"}
      }
    }
  ],
  "config": {}
}